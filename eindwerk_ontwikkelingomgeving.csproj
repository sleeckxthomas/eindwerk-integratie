    </Content>
    <Content Include="images\balcb.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="images\balcb2.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="images\balrood.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
