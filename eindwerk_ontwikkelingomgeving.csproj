<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="images\baland.jpg" />
    <None Remove="images\baland2.jpg" />
    <None Remove="images\balcb.jpg" />
    <None Remove="images\balcb2.jpg" />
    <None Remove="images\balrood.jpg" />
    <None Remove="images\balrood2.jpg" />
